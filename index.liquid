			}
			@keyframes stars {
				from {
					background-position: 0 -200px;
				}
				to {
					background-position: -1920px 0; /* Performance note - apparently TranslateY is better */
				}
			}
			@keyframes stars2 {
				from {
					background-position: 1920px -400px;
				}
				to {
					background-position: 0 0; /* Performance note - apparently TranslateY is better */
				}
			}
			.stars2:after {
				position: absolute;
				width: 100%;
				height: 100%;
				z-index: -4;
				content: "";
				background:  linear-gradient(to bottom, transparent 70%, #000 100%);
			}
			/* Globe */
			.globe {
