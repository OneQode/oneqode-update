---

metaTitle: OneQode â€¢ Media & Press Resources
metaDescription: The latest news and updates from OneQode. Here you'll find press releases, product updates, and a downloadable media pack.
changefreq: weekly
priority: 0.6
ogImage: og-image.jpg
pageSupertitle: Media Resources
pageTitle: OneQode Newsroom
pageSubtitle: Making Australia an internet leader.

---
{% include _header.liquid %}
		<style>
			@media only screen and (max-width: 48em) {
				section.hero {
					padding-top: 6rem;
				}
			}
			@media only screen and (min-width: 48em) {
				.hero {
					background-size: cover !important;
					background-position: bottom center !important;
				}
				.no-webp .hero {
					background: url("../img/bg/media-hd.jpg");
				}
				.webp .hero {
					background: url("../img/bg/media-4k.webp");
				}
			}
		</style>
		<section class="hero">
			<div class="container">
				<div class="row middle-xs">
					<div class="col-md-6">
						<p class="superheading">{{ pageSupertitle }}</p>
						<h1>{{ pageTitle }}<mark class="dot">.</mark></h1>
						<p class="subheading">{{ pageSubtitle }}</p>
					</div>
					<div class="col-md-4 col-md-offset-2">
						<div class="press-info box p-2 text-center sm-mt-4">
							<h2>Media Contact</h2>
							<p><a href="mailto:{{ global.mediaEmail }}">{{ global.mediaEmail }}</a></p>
							<h2 class="mt-2">Media Pack</h2>
							<p><small><a href="{{ global.mediaPackLink }}" class="button ghost" target="_blank">Download</a></small></p>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class="black py-4">
			<div class="container pb-0">
				<h2 class="mb-4">Latest Updates
					<a href="../media-feed.xml" class="rss-link" title="Subscribe to Media Releases RSS feed" target="_blank">
						<svg viewBox="0 0 16 16" class="bi bi-rss" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
							<path fill-rule="evenodd" d="M14 1H2a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
							<path d="M5.5 12a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z" />
							<path fill-rule="evenodd" d="M2.5 3.5a1 1 0 0 1 1-1c5.523 0 10 4.477 10 10a1 1 0 1 1-2 0 8 8 0 0 0-8-8 1 1 0 0 1-1-1zm0 4a1 1 0 0 1 1-1 6 6 0 0 1 6 6 1 1 0 1 1-2 0 4 4 0 0 0-4-4 1 1 0 0 1-1-1z" />
						</svg>
					</a>
				</h2>
			</div>
			<div class="container linked">
				<div class="row mobile-flex">
					<div class="col-md-8">
						{%- for post in collections.internal reversed -%}
						<a href="{{ post.url }}">
							<h3>{{ post.data.postTitle }}</h3>
							<p>{{ post.data.postType }}</p>
							<p>{{ post.date | readableDate }}</p>
						</a>
						{% else %}
						<p>No releases to display</p>
						{%- endfor -%}
					</div>
				</div>
			</div>
		</section>
		<section class="mid-to-footer py-4">
			<div class="container pb-0">
				<h2 class="mb-4">In the Media</h2>
			</div>
			<div class="container linked">
				<div class="row mobile-flex">
					<div class="col-md-8">
						{%- for item in media-mentions %}
						<a href="{{ item.url }}" target="_blank">
							<h3>{{ item.title }}</h3>
							<p>{{ item.outlet }}</p>
							<p>{{ item.date }}</p>
						</a>
						{% else %}
						<p>No releases to display</p>
						{%- endfor -%}
					</div>
				</div>
			</div>
		</section>
{% include _footer.liquid %}