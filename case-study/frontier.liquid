		.no-webp .hero {
			background: linear-gradient(to bottom, transparent 40%, #000),
						url("/img/bg/frontier-counter-strike-cs2-soldiers.png") bottom right,
						url("/img/bg/oneqode-frontier-pattern.png")
						#1b36ff;
			background-repeat: no-repeat, no-repeat, repeat, no-repeat;
			background-attachment: scroll, scroll, fixed, scroll;
			background-size: auto, contain, auto, auto;
		}
		.webp .hero {
			background: linear-gradient(to bottom, transparent 40%, #000),
						url("/img/bg/frontier-counter-strike-cs2-soldiers.webp") bottom right,
						url("/img/bg/oneqode-frontier-pattern.webp")
