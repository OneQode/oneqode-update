			/**
			* Botpoison form validation
			* Prevents form submissions from bots... I hope
			*/

			var form = document.getElementById("cloud-signup");
			var button = document.getElementById("button");
			var sending = document.getElementById("sending");
			var failmsg = document.getElementById("failmsg");
			form.addEventListener("botpoison-challenge-start", function () {
				button.setAttribute("disabled", "disabled");
				sending.classList.add("in-progress");
			});
