<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Page Info -->
	<title>{{ metaTitle }}</title>
	<meta property="og:title" content="{{ metaTitle }}" />
	<meta name="description" content="{{ metaDescription }}" />
	<meta property="og:description" content="{{ metaDescription }}" />
	<meta property="og:type" content="website" />
	<meta property="og:url" content="{{ global.url }}{{ page.url }}" />
	<meta property="og:site_name" content="OneQode" />
	<meta property="og:image" content="{{ global.url }}/img/og/{{ ogImage }}"/>
	<meta property="article:publisher" content="https://www.facebook.com/oneqode" />
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:description" content="{{ metaDescription }}">
	<meta name="twitter:image" content="{{ global.url }}/img/og/{{ ogImage }}" />
	<meta name="twitter:title" content="{{ metaTitle }}" />
	<meta name="twitter:site" content="@oneqode" />
	<meta name="twitter:creator" content="@oneqode" />
	{%- if ignore %}
	<meta name="robots" content="noindex">
	{%- endif %}
	{%- if canonical %}
	<link rel="canonical" href="{{ global.url }}{{ canonical }}">
	{%- endif %}
	<!-- Organisation Info -->
	<script type="application/ld+json">
		{
			"@context":"https://schema.org",
			"@type":"Organization",
			"name":"OneQode",
			"url":"https://www.oneqode.com/",
			"logo":"https://www.oneqode.com/img/symbol.png",
			"address":{
				"@type":"PostalAddress",
				"streetAddress":"3/38 High Street",
				"addressLocality":"Kippa Ring",
				"addressRegion":"Queensland",
				"postalCode":"4021",
				"addressCountry":"Australia"
			},
			"contactPoint":{
				"@type":"ContactPoint",
				"contactType":"contact",
				"telephone":"{{ global.contactNumber }}",
				"email":"{{ global.contactEmail }}"
			},
			"sameAs":[
				"https://www.linkedin.com/company/oneqode/",
				"https://twitter.com/oneqode"
			]
		}
	</script>
	<!-- Styles -->
	<link rel="stylesheet" href="{{ '/css/production.min.css' | url }}">
	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@600&family=Work+Sans:ital,wght@0,400;0,600;1,400&display=swap" rel="stylesheet">
	<!-- Shortcut Icons -->
	<link rel="icon" href="{{ '/favicon.svg' | url }}">
	<link rel="apple-touch-icon" href="{{ '/favicon.png' | url }}" />
	<meta name="msapplication-TileImage" content="{{ '/favicon.png' | url }}" />
	<!-- Status Bar -->
	<meta name="theme-color" content="#00b0f0">
	<meta name="msapplication-navbutton-color" content="#00b0f0">
	<meta name="apple-mobile-web-app-status-bar-style" content="#00b0f0">
	<!-- Google Tag Manager -->
	<script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
	new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
	j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
	'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
	})(window,document,'script','dataLayer','GTM-5FZ7DSW');</script>
	<!-- Scripts -->
	<script src="{{ '/js/production.js' | url }}"></script>
{%- if availabilityChecker %}
	<!-- Availability Checker -->
	<script defer src="{{ '/js/address-checker-conf.js' | url }}"></script>
	<script defer src="{{ '/js/address-checker.js' | url }}"></script>
	<link rel="stylesheet" href="/css/availability.css">
	{%- endif %}
{%- if form %}
{% include forms/__form-header.liquid %}
{%- endif %}
</head>
<body>
	<!-- Google Tag Manager (noscript) -->
	<noscript>
		<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-5FZ7DSW"
	height="0" width="0" style="display:none;visibility:hidden"></iframe>
	</noscript>
	<!-- End Google Tag Manager (noscript) -->
	<header id="header">
		<div class="container">
			<a href="
				{%- if page.url == "/" or page.url == "/business/" or page.url == "/wholesale/" -%}
					/
				{%- else -%}
					{%- case section -%}
						{%- when "home" -%}
							/
						{%- when "business" -%}
							/business/
						{%- when "wholesale" -%}
							/wholesale/
						{%- else -%}
							/
					{%- endcase -%}
				{%- endif -%}
				" title="
				{%- if page.url == "/" or page.url == "/business/" or page.url == "/wholesale/" -%}
					Go to the OneQode homepage
				{%- else -%}
					{%- case section -%}
						{%- when "home" -%}
							Go to the OneQode homepage
						{%- when "business" -%}
							Go to the OneQode for Business homepage
						{%- when "wholesale" -%}
							Go to the OneQode for Wholesale homepage
						{%- else -%}
							Go to the OneQode homepage
					{%- endcase -%}
				{%- endif -%}
				" class="logo">
				<img src="{{ '/img/logo.svg' | url }}" alt="OneQode Logo">
			</a>
{% case section %}
	{%- when "home" -%}
		{%- include navigation/_navigation-home.liquid -%}
	{%- when "business" -%}
		{%- include navigation/_navigation-business.liquid -%}
	{%- when "wholesale" -%}
		{%- include navigation/_navigation-wholesale.liquid -%}
	{%- else -%}
		{%- include navigation/_navigation-home.liquid -%}
{%- endcase -%}
		</div>
	</header>
