			/**
			* International Telephone Input
			* Validates international phone numbers
			* https://github.com/jackocnr/intl-tel-input
			*/
			var input = document.querySelector("#phone");
			window.intlTelInput(input, {
				geoIpLookup: function(callback) {
					$.get('https://ipinfo.io?token=9a0c888a083a9e', function() {}, "jsonp").always(function(resp) {
						var countryCode = (resp && resp.country) ? resp.country : "us";
						callback(countryCode);
					});
				},
				preferredCountries: ['us', 'au', 'cn', 'jp'],
				initialCountry: 'auto',
				separateDialCode: true,
				utilsScript: "https://unpkg.com/intl-tel-input@17.0.16/build/js/utils.js",
			});

			{%- comment %}
			/**
			* Show text if non-obvious gaming option selected 
			*/
			function typeCheck() {
				select = document.getElementById("company-type").value;
				if (select == "Other") {
					document.getElementById("please-note").style.display = "block";
				} else {
					document.getElementById("please-note").style.display = "none";
				}
			}
			{%- endcomment %}

			/**
			* Botpoison form validation
			* Prevents form submissions from bots... I hope
			*/

			var form = document.getElementById("cloud-signup");
			var button = document.getElementById("button");
			var sending = document.getElementById("sending");
			var failmsg = document.getElementById("failmsg");
			form.addEventListener("botpoison-challenge-start", function () {
				button.setAttribute("disabled", "disabled");
				sending.classList.add("in-progress");
			});
			form.addEventListener("botpoison-challenge-success", function () {
				button.removeAttribute("disabled");
				sending.classList.remove("in-progress");
				button.classList.add("success");
			});
			form.addEventListener("botpoison-challenge-error", function () {
				button.removeAttribute("disabled");
				button.classList.add("fail");
				sending.classList.remove("in-progress");
				failmsg.classList.add("block");
			});
		</script>