{% include _header.liquid %}
		<!-- Page-Specific Styles -->
		<style>
			/* Hero */
			.hero {
				background: none !important;
			}
			section.hero {
				padding-top: 6rem !important;
				min-height: 75vh;
			}
			/* Stars BG */
			.stars,
			.stars2 {
				width: 100%;
				height: 100%;
				position: absolute;
				z-index: -3;
			}
			.no-webp .stars,
			.no-webp .stars2 {
				background: url("/img/bg/stars-background.jpg") repeat;
				background-size: cover;
			}
			.webp .stars,
			.webp .stars2 {
				background: url("/img/bg/stars-background.webp") repeat;
				background-size: cover;
			}
			.stars {
				animation: stars 120s linear infinite;
			}
			.stars2 {
				animation: stars2 120s linear infinite;
			}
			@keyframes stars {
				from {
					background-position: 0 -200px;
				}
				to {
					background-position: -1920px 0; /* Performance note - apparently TranslateY is better */
				}
			}
			@keyframes stars2 {
				from {
					background-position: 1920px -400px;
				}
				to {
					background-position: 0 0; /* Performance note - apparently TranslateY is better */
				}
			}
			.stars2:after {
				position: absolute;
				width: 100%;
				height: 100%;
				z-index: -2;
				content: "";
				background:  linear-gradient(to bottom, transparent 70%, #000 100%);
			}
			/* Globe */
			.globe {
				margin-top: -16rem;
			}
			.globe .container {
				position: relative;
				display: block;
				width: 100%;
				padding: 0;
			}
			.cloud-locations {
				z-index: 3;
				position: absolute;
				width: 100%;
				height: 100%;
			}
			/*
				Pulses
				Credit to Florin Pop - https://www.florin-pop.com/blog/2019/03/css-pulse-effect/
			*/
			.pulse {
				border-radius: 50%;
				height: 1rem;
				width: 1rem;
				background: rgba(0, 176, 240, 1);
				box-shadow: 0 0 0 0 rgba(0, 176, 240, 1);
				animation: pulse 2s infinite;
				display: inline-block;
				position: absolute;
			}
			@keyframes pulse {
				0% {
					transform:
						scale(0.95)
						translate(-50%, -50%);
					box-shadow: 0 0 0 0 rgba(0, 176, 240, 0.7);
				}
				70% {
					transform:
						scale(1)
						translate(-50%, -50%);
					box-shadow: 0 0 0 10px rgba(0, 176, 240, 0);
				}
				
				100% {
					transform:
						scale(0.95)
						translate(-50%, -50%);
					box-shadow: 0 0 0 0 rgba(0, 176, 240, 0);
				}
			}
			.pulse.singapore {
				left: 34.7%;
				top: 52%;
			}
			.pulse.hong-kong {
				left: 43.5%;
				top: 45%;
			}
			.pulse.guam {
				left: 59.3%;
				top: 53.7%;
			}
			.pulse.tokyo {
				left: 58.7%;
				top: 42.7%;
			}
			.cloud-locations .label {
				position: absolute;
				text-align: center;
				font-family: Poppins, sans-serif;
				font-weight: 600;
				color: #27ade4;
				white-space: nowrap;
			}
			.cloud-locations .label span {
				position: absolute;
				transform: translateX(-50%);
			}
			/* Mobile Overlay Overrides */
			@media (max-width: 64em) {
				.cloud-locations .label span {
					transform: translateX(-50%) translateY(-50%);
				}
				span.pulse {
					height: 0.5rem;
					width: 0.5rem;
				}
			}
			.label.guam {
				left: 59.3%;
				top: 49.7%;
			}
			.label.singapore {
				left: 34.7%;
				top: 48%;
			}
			.label.hong-kong {
				left: 43.5%;
				top: 41%;
			}
			.label.tokyo {
				left: 58.7%;
				top: 38.7%;
			}
			.globe svg {
				position: absolute;
				z-index: 1;
				width: 100%;
			}
			.globe svg path {
				fill: none;
				stroke: #27ade4;
				stroke-width: 1;
				stroke-linecap: round;
				stroke-miterlimit: 10;
				stroke-dasharray: 6;
				stroke-dashoffset: 216;
				animation: globelines 2s linear infinite;
			}
			@keyframes globelines {
				100% {
					stroke-dashoffset: 0;
				}
			}
			.globe img {
				mix-blend-mode: lighten;
			}
			.globe picture {
				position: relative;
			}
			/* This stops the globe having stars on top (because of its mix blend mode) */
			.globe picture:after {
				position: absolute;
				top: 50%;
				left: 50%;
				transform: translate(-50%, -50%);
				width: 72.5%;
				height: 72.5%;
				border-radius: 50%;
				content: " ";
				background: #000;
				z-index: -1;
			}
			/* Servers */
			#servers {
				position: relative;
				overflow: hidden;
			}
			@media (max-width: 64em) {
				#servers img,
				#cloud img {
					margin-top: 4rem;
				}
				/* Hide Guam background */
				#cable-map,
				#cable-map::after {
					display: none !important;
				}
				#guam {
					min-height: 0 !important;
				}
			}
			@media (min-width: 64em) {
				#servers img {
					position: absolute;
					height: 400px;
					margin-top: -50px;
				}
			}
			#cloud img {
				box-shadow: 0px 0px 120px -20px #00b0f0;
				border-radius: 10px;
			}
			/* Guam Map BG */
			#guam {
				position: relative;
			}
			/* SVG Styles */
			#cable-map {
				position: absolute;
				z-index: -1;
				width: 100%;
				overflow: hidden;
				/*margin: -4rem 0 -16rem 0;*/
				margin:  -4rem 0 -10rem 0;
			}
			#cable-map::after {
				content: "";
				display: block;
				width: 100%;
				height: 100%;
				position: absolute;
				background: radial-gradient(closest-side, rgba(0,0,0,0.6) 60%, rgba(0,0,0,0.4) 80%, #000);
				top: 0;
				left: 0;
				z-index: 2;
			}			
			.cable {
				fill-rule: evenodd;
				clip-rule: evenodd;
				fill: none;
				stroke: #00B0F0;
				stroke-width: 4;
				stroke-dasharray: 15;
				stroke-dashoffset: 510;
				animation: lines 2s linear infinite;
			}
			@keyframes lines {
				100% {
					stroke-dashoffset: 0;
				}
			}
			#guam-island {
				fill-rule: evenodd;
				clip-rule: evenodd;
				fill: #004B66;
				stroke: #00B0F0;
			}
		</style>
