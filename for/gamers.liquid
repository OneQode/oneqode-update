		<script>
			// Organisation Type Switcher
			function orgType() {
				select = document.getElementById("organisation-type").value;
				// Select all interactive bits and hide them
				function hideStuff() {
					const interactive = Array.from(document.getElementsByClassName("interactive"));
						interactive.forEach(item => {
							item.style.display = "none";
						});
				}
				switch(select) {
					case "tournament-organiser":
						hideStuff();
						document.getElementById("tournament-organiser").style.display = "flex";
						break;
					case "esports-dev-publisher":
						hideStuff();
						document.getElementById("esports-dev-publisher").style.display = "flex";
						break;
					case "college-esports":
						hideStuff();
						document.getElementById("college-esports").style.display = "flex";
						break;
					case "player":
						hideStuff();
						document.getElementById("player").style.display = "flex";
						break;
				}
			}
		</script>
