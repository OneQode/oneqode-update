		<script>
			/**
			* International Telephone Input
			* Validates international phone numbers
			* https://github.com/jackocnr/intl-tel-input
			*/
			var input = document.querySelector("#phone");
			window.intlTelInput(input, {
				geoIpLookup: function(callback) {
					$.get('https://ipinfo.io?token=9a0c888a083a9e', function() {}, "jsonp").always(function(resp) {
						var countryCode = (resp && resp.country) ? resp.country : "us";
						callback(countryCode);
					});
				},
				preferredCountries: ['us', 'au', 'cn', 'jp'],
				initialCountry: 'auto',
				separateDialCode: true,
				utilsScript: "https://unpkg.com/intl-tel-input@17.0.16/build/js/utils.js",
			});
			/* Submit country code in hidden field */
				// Get the instance of the IntlTelInput
				var iti = window.intlTelInputGlobals.getInstance(input);
				// Function to update the hidden input value with the selected country code
				function updateCountryCode() {
					var countryCode = iti.getSelectedCountryData().dialCode;
					document.getElementById("phonecountrycode").value = countryCode;
				}
				// Listen to the countrychange event on the phone input
				input.addEventListener("countrychange", updateCountryCode);
				// Update the country code initially in case the user doesn't change the country
				updateCountryCode()
		</script>