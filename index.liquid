			.cloud-locations {
				z-index: 1;
				position: absolute;
				width: 100%;
				height: 100%;
			}
				/*
					Pulses
					Credit to Florin Pop - https://www.florin-pop.com/blog/2019/03/css-pulse-effect/
				*/
			.pulse {
				border-radius: 50%;
				height: 0.7rem;
				width: 0.7rem;
				background: rgba(0, 176, 240, 1);
				box-shadow: 0 0 0 0 rgba(0, 176, 240, 1);
				animation: pulse 2s infinite;
				display: inline-block;
				position: absolute;
			}
			@keyframes pulse {
				0% {
					transform:
						scale(0.95)
						translate(-50%, -50%);
					box-shadow: 0 0 0 0 rgba(0, 176, 240, 0.7);
				}
				70% {
					transform:
						scale(1)
						translate(-50%, -50%);
					box-shadow: 0 0 0 10px rgba(0, 176, 240, 0);
				}
				
				100% {
					transform:
						scale(0.95)
						translate(-50%, -50%);
					box-shadow: 0 0 0 0 rgba(0, 176, 240, 0);
				}
			}
			.pulse.singapore {
				left: 34.7%;
				top: 52%;
			}
			.pulse.hong-kong {
				left: 43.5%;
				top: 45%;
			}
			.pulse.guam {
