				font-size: 0.75rem;
				margin-bottom: 0.5rem;
				margin-top: 0;
			}
			#api code:last-of-type {
				margin-bottom: 0;
			}
			.bold {
				 font-weight: bold;
			}
			.italic {
				font-style: italic;
			}
			.code1 {
				color: #ababab;
			}
			.code2 {
				color: #00b0f0;
			}
			.code3 {
				color: #ff8c8c;
			}
			.code4 {
				color: #999;
			}
			.code5 {
				color: #ff7100;
			}
			.code6 {
				color: #ffb9b9;
			}
			.code7 {
				color: #35ff7c;
			}
			.code8 {
				color: #BB6688;
			}
