		<script>
			// The Swiper / Slider
			var swiper = new Swiper(".benefits", {
				effect: "coverflow",
				grabCursor: true,
				slidesPerView: "auto",
				centeredSlides: true,
				centeredSlidesBounds: true,
				coverflowEffect: {
					rotate: 50,
					stretch: 0,
					depth: 100,
					modifier: 0.8,
					slideShadows: true,
				},
				loop: true,
				pagination: {
					el: ".swiper-pagination",
					clickable: true,
				},
				navigation: {
					nextEl: ".swiper-button-next",
					prevEl: ".swiper-button-prev",
				},
				keyboard: {
					enabled: true,
				},
			});
			// Organisation Type Switcher
			function orgType() {
				select = document.getElementById("organisation-type").value;
				// Select all interactive bits and hide them
				function hideStuff() {
					const interactive = Array.from(document.getElementsByClassName("interactive"));
						interactive.forEach(item => {
							item.style.display = "none";
						});
				}
				switch(select) {
					case "player-benefits":
						hideStuff();
						document.getElementById("player-benefits").style.display = "flex";
						break;
					case "developer-benefits":
						hideStuff();
						document.getElementById("developer-benefits").style.display = "flex";
						break;
					case "publisher-benefits":
						hideStuff();
						document.getElementById("publisher-benefits").style.display = "flex";
						break;
				}
			}
