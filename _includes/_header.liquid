<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Page Info -->
	<title>{{ metaTitle }}</title>
	<meta property="og:title" content="{{ metaTitle }}" />
	<meta name="description" content="{{ metaDescription }}" />
	<meta property="og:description" content="{{ metaDescription }}" />
	<meta property="og:type" content="website" />
	<meta property="og:url" content="{{ global.url }}{{ page.url }}" />
	<meta property="og:site_name" content="OneQode" />
	<meta property="og:image" content="{{ global.url }}/img/og/{{ ogImage }}"/>
	<meta property="article:publisher" content="https://www.facebook.com/oneqode" />
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:description" content="{{ metaDescription }}">
	<meta name="twitter:image" content="{{ global.url }}/img/og/{{ ogImage }}" />
	<meta name="twitter:title" content="{{ metaTitle }}" />
	<meta name="twitter:site" content="@oneqode" />
	<meta name="twitter:creator" content="@oneqode" />
	{%- if ignore %}
	<meta name="robots" content="noindex">
	{%- endif %}
	{%- if canonical %}
	<link rel="canonical" href="{{ global.url }}{{ canonical }}">
	{%- endif %}
	<!-- Organisation Info -->
	<script type="application/ld+json">
		{
			"@context":"https://schema.org",
			"@type":"Organization",
			"name":"OneQode",
			"url":"https://www.oneqode.com/",
			"logo":"https://www.oneqode.com/img/symbol.png",
			"address":{
				"@type":"PostalAddress",
				"streetAddress":"3/38 High Street",
				"addressLocality":"Kippa Ring",
				"addressRegion":"Queensland",
				"postalCode":"4021",
				"addressCountry":"Australia"
			},
			"contactPoint":{
				"@type":"ContactPoint",
				"contactType":"contact",
				"telephone":"{{ global.contactNumber }}",
				"email":"{{ global.contactEmail }}"
			},
			"sameAs":[
				"https://www.linkedin.com/company/oneqode/",
				"https://twitter.com/oneqode"
			]
		}
	</script>
	<!-- Styles -->
	<link rel="stylesheet" href="{{ '/css/production.min.css' | url }}">
	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@600&family=Work+Sans:ital,wght@0,400;0,600;1,400&display=swap" rel="stylesheet">
	<!-- Shortcut Icons -->
	<link rel="icon" href="{{ '/favicon.svg' | url }}">
	<link rel="apple-touch-icon" href="{{ '/favicon.png' | url }}" />
	<meta name="msapplication-TileImage" content="{{ '/favicon.png' | url }}" />
	<!-- Status Bar -->
	<meta name="theme-color" content="#00b0f0">
	<meta name="msapplication-navbutton-color" content="#00b0f0">
	<meta name="apple-mobile-web-app-status-bar-style" content="#00b0f0">
