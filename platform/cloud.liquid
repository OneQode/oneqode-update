{% include _header.liquid %}
		<style>
			.hero {
				background: none !important;
				padding-top: 6rem !important;
				height: 100%;
			}
			.hero img {
				box-shadow: 0px 0px 120px -20px #00b0f0;
				border-radius: 10px;
			}
			/* Fast text */
			.fast-text h2 {
				font-size: 7.4vw;
				line-height: 1;
			}
			.fast-text .subheading {
				margin-top: 0;
			}
			/* CPU BG */
			.no-webp .cpu-bg {
				background: radial-gradient(circle at 40% 50%,transparent 0%, rgba(0,0,0,0.3) 50%, #000 100%) no-repeat, url("/img/bg/ryzen-cpu.jpg") no-repeat 0% 33%;
			}
			.webp .cpu-bg {
				background: radial-gradient(circle at 40% 50%,transparent 0%, rgba(0,0,0,0.3) 50%, #000 100%) no-repeat, url("/img/bg/ryzen-cpu.webp") no-repeat 0% 33%;
			}
			/* CPU text */
			#cpu .superheading {
				margin-bottom: 0;
			}
			#cpu .subheading {
				line-height: 1;
			}
			#cpu .subheading .num {
				font-size:  8rem;
			}
			#cpu .subheading .unit {
				font-size:  6rem;
			}
			/* Network Earth Image */
			.cpu-bg {
				overflow-x: clip; /* Stops earth image overflowing */
			}
			#network {
				position: relative;
				overflow: visible;
			}
			/* Blobs BG */
			.bg-blobs {
				background:
					radial-gradient(circle at 100% 35%,#002735 0%,transparent 25%),
					radial-gradient(circle at 0% 65%,#002735 0%,transparent 25%);
			}
			.code-container {
				background: #333;
				padding: 1rem;
				border-radius: 10px;
				height: 300px;
				overflow-y: scroll;
			}
			/* Scrollbars credit: https://codepen.io/devstreak/pen/dMYgeO?editors=0100 */
			.code-container::-webkit-scrollbar-track {
				-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
				border-radius: 10px;
				background-color: none;
			}
			.code-container::-webkit-scrollbar {
				width: 12px;
				background-color: none;
			}
			.code-container::-webkit-scrollbar-thumb {
				border-radius: 10px;
				-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
				background-color: #555;
			}
			#api pre {
				display: grid;
				margin-top: 0;
				margin-bottom: 0;
			}
			#api code {
				font-family: Hack, monospace;
				font-size: 0.75rem;
				margin-bottom: 0.5rem;
				margin-top: 0;
			}
			#api code:last-of-type {
				margin-bottom: 0;
			}
			.bold {
				 font-weight: bold;
			}
			.italic {
				font-style: italic;
			}
			.code1 {
				color: #ababab;
			}
			.code2 {
				color: #00b0f0;
			}
			.code3 {
				color: #ff8c8c;
			}
			.code4 {
				color: #999;
			}
			.code5 {
				color: #ff7100;
			}
			.code6 {
				color: #ffb9b9;
			}
			.code7 {
				color: #35ff7c;
			}
			.code8 {
				color: #BB6688;
			}
			#management {
				overflow: hidden;
			}
			/*  Logo Grid  */
			.logo-grid {
				display: grid;
				grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));
				grid-gap: 1rem;
			}
			@media (min-width: 48em) {
				.logo-grid {
					grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));
					grid-gap: 1rem;
				}
			}
			@media (max-width: 340px) {
				.logo-grid {
					grid-template-columns: repeat(auto-fill, minmax(70px, 1fr));
					grid-gap: 0.2rem;
				}
			}
			.logo-grid > div {
				padding: 1rem;
				display: grid;
				place-items: center;
			}
			.logo-grid > div::before {
				content: "";
				display: block;
				padding-bottom: 75%;
				grid-area: 1/1/2/2;
			}
			.logo-grid > div img {
				width: 100%;
				grid-area: 1/1/2/2;
				filter: brightness(0) invert(1);
				opacity: 0.3;
				transition: opacity 0.2s;
			}
			.logo-grid > div:hover img {
				opacity: 0.8;
			}
			/* ↓ Responsive Styles ↓ */
			@media (min-width: 1920px) {
				.webp .cpu-bg, .no-webp .cpu-bg {
					background-size: contain;
				}
			}
			@media (max-width: 1920px) {
				.webp .cpu-bg, .no-webp .cpu-bg {
					background-size: 75%;
				}
			}
			@media (min-width: 75em) {
				h1 {
					font-size: 3.5rem !important;
				}
			}
			@media (min-width: 64em) {
				h1 {
					font-size: 3rem;
				}
				#network {
					padding-top: 16rem;
				}
				#network img {
					position: absolute;
					height: 800px;
					margin-top: -200px;
					border-radius: 50%;
				}
			}
			@media (max-width: 64em) {
				.webp .cpu-bg {
					background: radial-gradient(circle at 40% 50%,transparent 0%, rgba(0,0,0,0.3) 50%, #000 100%) no-repeat, url("/img/bg/ryzen-cpu.webp") no-repeat 0% 18%;
				}
				.no-webp .cpu-bg {
					background: radial-gradient(circle at 40% 50%,transparent 0%, rgba(0,0,0,0.3) 50%, #000 100%) no-repeat, url("/img/bg/ryzen-cpu.webp") no-repeat 0% 18%;
				}
				.webp .cpu-bg, .no-webp .cpu-bg {
					background-size: contain;
				}
				.hero img {
					margin-top: 4rem;
				}
				.fast-text h2, #cpu .subheading .num {
					font-size: 16vw;
				}
				#cpu .subheading .unit {
					font-size: 14vw;
				}
				#network img {
					margin-top: 4rem;
					margin-bottom: -8rem;
				}
				#ddos-protection div:first-child, #api img {
					margin-bottom: 4rem;
				}
			}
			@media (min-width: 48em) {
				/* Map */
				.pull-bottom {
					margin-bottom: -16rem;
					z-index: 2;
					position: relative;
				}
				/* Management Background */
				.no-webp #management {
					background: url("/img/bg/monitor-with-cloud-dashboard-open.jpg") no-repeat center right;
					background-size: contain;
				}
				.webp #management {
					background: url("/img/bg/monitor-with-cloud-dashboard-open.webp") no-repeat center right;
					background-size: contain;
				}
			}
			@media (min-width: 48em) and (max-width: 64em) {
				h1 {
					font-size: 2.5rem;
				}
				/* Management Background */
				.no-webp #management {
					background: radial-gradient(at top left, #000, rgba(0,0,0,0.4)), url("/img/bg/monitor-with-cloud-dashboard-open.jpg") no-repeat center right;
					background-size: contain;
				}
				.webp #management {
					background: radial-gradient(at top left, #000, rgba(0,0,0,0.4)), url("/img/bg/monitor-with-cloud-dashboard-open.webp") no-repeat center right;
					background-size: contain;
				}
			}
			@media (max-width: 48em) {
				
			}
		</style>
