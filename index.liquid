{% include _header.liquid %}
	<!-- Page-Specific Styles -->
	<style>
		.no-webp .hero {
			background: linear-gradient(to bottom, transparent 70%, #000), url("/img/bg/star-header.jpg") no-repeat center center;
			background-size: cover;
		}
		.webp .hero {
			background: linear-gradient(to bottom, transparent 70%, #000), url("/img/bg/star-header.webp") no-repeat center center;
			background-size: cover;
		}
		section.hero {
			padding-top: 6rem !important;
			height: 100%;
		}
		/* Particles JS */
		canvas {
			position: absolute;
			z-index: 0;
			height:  80vh !important;
		}
		section.hero::after {
			position: absolute;
			width: 100%;
			height: 80vh;
			content: " ";
			background: linear-gradient(to bottom, transparent 70%, #000);
		}
		/* Kotaku Article Quote */
		.kotaku-quote blockquote p {
			font-family: "Poppins", sans-serif;
			font-weight: 600;
			font-size: 1.5rem;
		}
		a.kotaku-logo {
			display: block;
			width: min(20rem, 60%);
			margin: 0 auto;
			opacity: 0.8;
		}
		a.kotaku-logo:hover,
		a.kotaku-logo:focus {
			opacity: 1;
		}
		.no-webp #network {
			background: url(/img/glowing-oneqode-network-globe.jpg) no-repeat 130% 50%;
			background-size: contain;
		}
		.webp #network {
			background: url(/img/glowing-oneqode-network-globe.webp) no-repeat 130% 50%;
			background-size: contain;
		}
		#servers {
			position: relative;
			overflow: hidden;
		}
		@media (max-width: 64em) {
			#servers img {
				margin-top: 4rem;
			}
		}
		@media (min-width: 64em) {
			#servers img {
				position: absolute;
				height: 400px;
				margin-top: -50px;
			}
		}
		/* Typer.js - Credit - https://github.com/qodesmith/typer */
		.typer {
			display: inline-block;
		}
		.typer::after {
		  content: '';
		  display: inline-block;
		  vertical-align: sub;
		  width: 0.1em;
		  height: 1.2em;
		  background-color: currentColor;
		  margin-left: 0.1em;
		}

		.cursor-block::after {
		  width: 0.5em;
		}

		.cursor-soft::after {
		  animation: softblink 0.7s infinite;
		}

		.cursor-hard::after {
		  animation: hardblink 0.7s infinite;
		}

		.no-cursor::after {
		  content: none;
		}

		.white-space {
		  white-space: pre-wrap;
		}

		@keyframes softblink {
		  0% {
		    opacity: 1;
		  }
		  50% {
		    opacity: 0;
		  }
		  100% {
		    opacity: 1;
		  }
		}

		@keyframes hardblink {
		  50%,
		  100% {
		    visibility: hidden;
		  }
		}
		/* Hide Typed Content */
		span[class^="typed-content-"] {
			display: none;
		}
	</style>
