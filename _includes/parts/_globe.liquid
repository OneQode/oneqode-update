				.globe svg {
					position: absolute;
					z-index: 0;
					width: 100%;
				}
				.globe svg path {
					fill: none;
					stroke: #27ade4;
					stroke-width: 1;
					stroke-linecap: round;
					stroke-miterlimit: 10;
					stroke-dasharray: 6;
					stroke-dashoffset: 216;
					animation: globelines 2s linear infinite;
				}
				@keyframes globelines {
					100% {
						stroke-dashoffset: 0;
					}
				}
				.globe img {
					mix-blend-mode: lighten;
				}
				.globe picture {
					position: relative;
				}
				/* This stops the globe having stars on top (because of its mix blend mode) */
				.globe picture:after {
					position: absolute;
					top: 50%;
					left: 50%;
					transform: translate(-50%, -50%);
					width: 72.5%;
					height: 72.5%;
					border-radius: 50%;
					content: " ";
					background: #000;
					z-index: -1;
				}
				/* Mobile Globe Overrides */
				@media (max-width: 64em) {
					.globe-locations .label span {
						transform: translateX(-50%) translateY(-50%);
						font-size: 65%;
					}
					.globe-locations .label:not(.cloud) {
						font-size: 46%;
					}
					span.pulse {
						height: 0.5rem;
						width: 0.5rem;
					}
					.globe svg path {
						/*stroke-width: 4;
						stroke-dasharray: 20;
						stroke-dashoffset: 400;*/
						stroke-width: 2;
						stroke-dasharray: 15;
						stroke-dashoffset: 390;
					}
				}
